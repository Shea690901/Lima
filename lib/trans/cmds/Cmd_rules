du		[dir]
ed		[file|fname]
find -in        str file*
force		user str*
grep -in	str file*
loadall		dir
ls -aslF	[file|dir*]
mkdir		fname
more		file*
mv -rf		file* fname|dir|file
update	-rR	[cfile*]
replace	-qn	str str file*
rm		file*
rmdir		dir*
setlevel	user num
snoop		[user] [str*]
spew		file str*
stat		obj|user
tail		file*
